{
  "presets": [
    ["@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": "3.33.0"
      }]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-modules-systemjs",
      {
        // outputs SystemJS.register(...)
        "systemGlobal": "System"
      }
    ]
  ],
  "ignore": [
    "**/node_modules/**",
    "**/font-awesome/**"
  ]
}  